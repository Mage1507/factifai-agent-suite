{
  "name": "@presidio-dev/playwright-core",
  "version": "1.0.3",
  "description": "A powerful wrapper around Playwright providing browser automation services with enhanced capabilities for LLMs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "node esbuild.config.js && tsc --emitDeclarationOnly",
    "build:watch": "node esbuild.config.js --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "postinstall": "npx playwright install --with-deps"
  },
  "dependencies": {
    "playwright": "^1.52.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "esbuild": "^0.20.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "playwright",
    "browser",
    "automation",
    "testing",
    "screenshot",
    "ui-testing",
    "ai-powered-testing"
  ],
  "author": "PRESIDIOÂ®",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/presidio-oss/factifai-agent-suite.git"
  },
  "packageManager": "pnpm@10.11.0"
}
